generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Projeto {
  id          Int      @id @default(autoincrement())
  titulo      String
  descricao   String
  tecnologias String[]
  imagemUrl   String
  linkProjeto String
  linkCodigo  String
}
